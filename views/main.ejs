<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>news</title>
    <link rel="stylesheet" href="css/main.css">



</head>
<body>

<div class="wrap">

    <header>
        <ul>
            <li>전체 언론사</li>
            <li>My news</li>
        </ul>
        <div class="btn">
            <span class="paging"> <%=page.currentPage%> / <%=page.totalPage%> </span>
            <div class="left"><a href="#"><</a></div>
            <div class="right"><a href="#">></a></div>
        </div>
    </header>

    <section class="mainArea" id="mainArea">
        <div id="titleList">
            <nav class="titleListNav">
                <ul class="titleUl">
                    <%pressList.forEach((val)=>{%>
                    <li><%=val%></li>
                    <%})%>
                </ul>
            </nav>
        </div>

        <section class="content" id="newsContents">


            <h4 class="title">
                <hidden class=<%=newsData._id%>></hidden>
                <span class="newsName"><%=newsData.press%></span> <button><a href="#">X</a></button>
            </h4>
            <div class="newsList">
                <img src="<%=newsData.imgurl%>" alt="">
                <ul>
                    <%newsData.newslist.forEach(function(val){%>
                        <li><%=val%></li>
                    <%})%>
                </ul>
            </div>

        </section>
    </section>
</div>



</body>
</html>
